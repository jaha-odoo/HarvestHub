<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_harvest_vehicle_form" model="ir.ui.view">
        <field name="name">view.harvest.vehicle.form</field>
        <field name="model">harvest.vehicle</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="quantity"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_harvest_vehicle_tree" model="ir.ui.view">
        <field name="name">view.harvest.vehicle.tree</field>
        <field name="model">harvest.vehicle</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="quantity"/>
            </tree>
        </field>
    </record>
</odoo>
